<form action="/create" method="POST">
  <h2>Create user</h2>

  <div class="input-field">
    <input type="text" name="name">
    <label>User title</label>
  </div>
  <div class="input-field">
    <input type="text" name="email">
    <label>User email</label>
  </div>

  <button type="submit" class="btn">Create</button>
</form>
